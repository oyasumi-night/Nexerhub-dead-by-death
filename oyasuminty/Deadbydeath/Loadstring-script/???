if game:GetService("MarketplaceService"):UserOwnsGamePassAsync(game:GetService("Players").LocalPlayer.UserId, 7099384011) then
    DiscordLib:Notification("Error!", "Arleady have gamepass!", "Okay!")
else
    OpenLink("https://www.roblox.com/game-pass/1264479709")
end
end; })

